<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Minsal\ModeloBundle\Entity\CtlProveedor" table="ctl_proveedor">
    <id name="id" type="integer" column="id">
      <generator strategy="SEQUENCE"/>
      <sequence-generator sequence-name="ctl_proveedor_id_seq" allocation-size="1" initial-value="1"/>
    </id>
    <field name="codigoProveedor" type="string" column="codigo_proveedor" length="50" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="nombreProveedor" type="string" column="nombre_proveedor" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="estadoProveedor" type="integer" column="estado_proveedor" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="nit" type="string" column="nit" length="100" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
