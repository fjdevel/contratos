<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Minsal\ModeloBundle\Entity\CtlIncremento" table="ctl_incremento">
    <indexes>
      <index name="fki_incremento_modalidad" columns="numero_modalidad_compra"/>
      <index name="fki_estado_incremento" columns="estado_incremento"/>
      <index name="fki_programacion_incremento" columns="estimacion"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="SEQUENCE"/>
      <sequence-generator sequence-name="ctl_incremento_id_seq" allocation-size="1" initial-value="1"/>
    </id>
    <field name="mesesDesestimar" type="integer" column="meses_desestimar" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="fechaCreacion" type="date" column="fecha_creacion" nullable="true"/>
    <many-to-one field="numeroModalidadCompra" target-entity="CtlModalidadCompra" fetch="LAZY">
      <join-columns>
        <join-column name="numero_modalidad_compra" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="estadoIncremento" target-entity="CtlEstados" fetch="LAZY">
      <join-columns>
        <join-column name="estado_incremento" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="estimacion" target-entity="CtlProgramacion" fetch="LAZY">
      <join-columns>
        <join-column name="estimacion" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
