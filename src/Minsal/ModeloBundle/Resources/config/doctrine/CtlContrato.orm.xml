<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Minsal\ModeloBundle\Entity\CtlContrato" table="ctl_contrato">
    <indexes>
      <index name="fki_contrato_modalidad" columns="numero_modalidad"/>
      <index name="fki_prove_contrat" columns="id_proveedor"/>
      <index name="fki_contrato_establecimiento" columns="establecimiento"/>
    </indexes>
    <id name="id" type="integer" column="id"/>
    <id name="establecimiento" association-key="true"/>
    <id name="idProveedor" association-key="true"/>
    <field name="montoContrato" type="decimal" column="monto_contrato" precision="16" scale="4" nullable="false"/>
    <field name="numeroContrato" type="string" column="numero_contrato" length="50" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <one-to-one field="establecimiento" target-entity="CtlEstablecimiento" fetch="LAZY">
      <join-columns>
        <join-column name="establecimiento" referenced-column-name="id"/>
      </join-columns>
    </one-to-one>
    <one-to-one field="idProveedor" target-entity="CtlProveedor" fetch="LAZY">
      <join-columns>
        <join-column name="id_proveedor" referenced-column-name="id"/>
      </join-columns>
    </one-to-one>
    <many-to-one field="numeroModalidad" target-entity="CtlModalidadCompra" fetch="LAZY">
      <join-columns>
        <join-column name="numero_modalidad" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
