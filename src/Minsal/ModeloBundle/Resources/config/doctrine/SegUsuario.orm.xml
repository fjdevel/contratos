<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Minsal\ModeloBundle\Entity\SegUsuario" table="seg_usuario">
    <id name="id" type="integer" column="id">
      <generator strategy="SEQUENCE"/>
      <sequence-generator sequence-name="seg_usuario_id_seq" allocation-size="1" initial-value="1"/>
    </id>
    <field name="nombre" type="text" column="nombre" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="password" type="string" column="password" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="tokenSinab" type="string" column="token_sinab" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="establecimientoid" type="integer" column="establecimientoid" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="fechaCreacion" type="date" column="fecha_creacion" nullable="false"/>
    <field name="fechaModificacion" type="date" column="fecha_modificacion" nullable="false"/>
    <field name="salt" type="text" column="salt" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <many-to-many field="idSegRol" target-entity="SegRol" inversed-by="segUsuarioid" fetch="LAZY">
      <join-table name="seg_usuarios_roles">
        <join-columns>
          <join-column name="seg_usuarioid" referenced-column-name="id"/>
        </join-columns>
        <inverse-join-columns>
          <join-column name="id_seg_rol" referenced-column-name="id"/>
        </inverse-join-columns>
      </join-table>
    </many-to-many>
  </entity>
</doctrine-mapping>
