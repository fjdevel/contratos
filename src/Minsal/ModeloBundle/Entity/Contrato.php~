<?php

namespace Minsal\ModeloBundle\Entity;

use Doctrine\ORM\Mapping as ORM;
use Doctrine\Common\Collections\ArrayCollection;


/**
 * Contrato
 *
 * @ORM\Table(name="contrato")
 * @ORM\Entity(repositoryClass="Minsal\ModeloBundle\Repository\ContratoRepository")
 */
class Contrato
{
    /**
     * @var int
     *
     * @ORM\Column(name="id", type="integer")
     * @ORM\Id
     * @ORM\GeneratedValue(strategy="AUTO")
     */
    private $id;

    /**
     * @ORM\OneToMany(targetEntity="Proveedor", mappedBy="contrato")
     */
    protected $proveedores;

    public function __construct()
    {
        $this->proveedores = new ArrayCollection();
    }


    /**
     * @ORM\ManyToOne(targetEntity="Licitacion", inversedBy="contratos")
     * 
     */
    protected $licitacion;


    /**
     * @var string
     *
     * @ORM\Column(name="numero_contrato", type="string", length=255)
     */
    private $numeroContrato;

    /**
     * @var float
     *
     * @ORM\Column(name="monto_contrato", type="float")
     */
    private $montoContrato;


    /**
     * Get id
     *
     * @return integer 
     */
    public function getId()
    {
        return $this->id;
    }

    /**
     * Set numeroContrato
     *
     * @param string $numeroContrato
     * @return Contrato
     */
    public function setNumeroContrato($numeroContrato)
    {
        $this->numeroContrato = $numeroContrato;

        return $this;
    }

    /**
     * Get numeroContrato
     *
     * @return string 
     */
    public function getNumeroContrato()
    {
        return $this->numeroContrato;
    }

    /**
     * Set montoContrato
     *
     * @param float $montoContrato
     * @return Contrato
     */
    public function setMontoContrato($montoContrato)
    {
        $this->montoContrato = $montoContrato;

        return $this;
    }

    /**
     * Get montoContrato
     *
     * @return float 
     */
    public function getMontoContrato()
    {
        return $this->montoContrato;
    }

    /**
     * Add proveedores
     *
     * @param \Minsal\ModeloBundle\Entity\Proveedor $proveedores
     * @return Contrato
     */
    public function addProveedore(\Minsal\ModeloBundle\Entity\Proveedor $proveedores)
    {
        $this->proveedores[] = $proveedores;

        return $this;
    }

    /**
     * Remove proveedores
     *
     * @param \Minsal\ModeloBundle\Entity\Proveedor $proveedores
     */
    public function removeProveedore(\Minsal\ModeloBundle\Entity\Proveedor $proveedores)
    {
        $this->proveedores->removeElement($proveedores);
    }

    /**
     * Get proveedores
     *
     * @return \Doctrine\Common\Collections\Collection 
     */
    public function getProveedores()
    {
        return $this->proveedores;
    }

    /**
     * Set licitacion
     *
     * @param \Minsal\ModeloBundle\Entity\Licitacion $licitacion
     * @return Contrato
     */
    public function setLicitacion(\Minsal\ModeloBundle\Entity\Licitacion $licitacion = null)
    {
        $this->licitacion = $licitacion;

        return $this;
    }

    /**
     * Get licitacion
     *
     * @return \Minsal\ModeloBundle\Entity\Licitacion 
     */
    public function getLicitacion()
    {
        return $this->licitacion;
    }
}
