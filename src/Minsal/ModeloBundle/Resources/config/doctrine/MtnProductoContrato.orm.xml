<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Minsal\ModeloBundle\Entity\MtnProductoContrato" table="mtn_producto_contrato">
    <indexes>
      <index name="fki_mtn_contrato" columns="mtn_contrato"/>
      <index name="fki_mtn_proveedor" columns="mtn_proveedor"/>
      <index name="fki_mtn_producto" columns="mtn_producto"/>
      <index name="idx_11667261736a527d" columns="mtn_establecimiento"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="SEQUENCE"/>
      <sequence-generator sequence-name="mtn_producto_contrato_id_seq" allocation-size="1" initial-value="1"/>
    </id>
    <field name="mtnContrato" type="integer" column="mtn_contrato" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="cantidad" type="decimal" column="cantidad" precision="16" scale="4" nullable="true"/>
    <field name="precioUnitario" type="decimal" column="precio_unitario" precision="8" scale="2" nullable="true"/>
    <field name="renglon" type="integer" column="renglon" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <many-to-one field="mtnProveedor" target-entity="CtlProveedor" fetch="LAZY">
      <join-columns>
        <join-column name="mtn_proveedor" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="mtnEstablecimiento" target-entity="CtlEstablecimiento" fetch="LAZY">
      <join-columns>
        <join-column name="mtn_establecimiento" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="mtnProducto" target-entity="CtlProducto" fetch="LAZY">
      <join-columns>
        <join-column name="mtn_producto" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
